{% extends "base.html.twig" %}

        {% block stylesheets %}
        {{ parent() }}
        <link rel="stylesheet" href="style/solicitudes.css">
        {% endblock %}
{% block body %}
<table>
<tr>
<th> Nombre</th>
<th> Email</th>
<th> Ciudad</th>
</tr>
<tr>
{% for nombre in nombres %}
<td> <p> <span>{{nombre.nombre}}</span></p> </td>
<td> <p> <span>{{nombre.email}}</span></p> </td>
<td> <p> <span>{{nombre.ciudad}}</span> </p></td>

{% endfor %}
</tr>
</table>
{% endblock %}
